<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.kosmo.mapper.MailingMapper">

<select id="mailsending" resultType="com.kosmo.vo.EmailVO">
	select mmail
	from member
	where mmail is not null
</select>

</mapper>
